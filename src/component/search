

const Search = ({seaechs , titleSearch}) => {
   
    const handel = (event) =>{
      
      seaechs(event);
    }
    return(
  <div>
  <label htmlFor="search">Search</label>
    <input onChange={handel} type="text" id="search" value={titleSearch} />
 
  </div>    
    )
  }
  export default Search;
