

const Search = ({  value , type , id ,seaechs , titleSearch , label,autoFo}) => {
   
    const handel = (event) =>{
      
      seaechs(event);
    }
    return(
 <>
  <label htmlFor={id}>{label}</label>
    <input onChange={handel} type={type} id={id} value={titleSearch} autoFocus={autoFo} />
 </>

 
 
    )
    
  }
  export default Search;
